    "chalk": "^2.4.1",
    "discord-rpc": "^3.0.0",
    "electron-config": "^1.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.16.3",
    "fs": "0.0.1-security",
    "html-entities": "^1.2.1",
    "menubar": "^5.2.3",
    "request": "^2.87.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.22",
    "@electron-forge/maker-deb": "^6.0.0-beta.22",
    "@electron-forge/maker-rpm": "^6.0.0-beta.22",
